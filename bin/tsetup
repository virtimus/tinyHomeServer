#!/bin/bash


e=0
msg=''
thsHome=/ths/tinyHomeServer
tstep=1

handleError(){
  if [ $e -ne 0 ];then echo "[ths-shell-init] step $tstep error $e" && exit; fi && ((tstep++))
}

handleHeader(){
	echo "[tsetup] $msg ..."
	tstep=1
}

handleEnd(){
	echo "[tsetup] END."
}



case "$2" in

  features/jupyter)
    source $thsHome/features/jupyter/ths-setup
		;;

  features/hass)
    source $thsHome/features/hass/ths-setup
		;;
	*)
			echo "Basic usage: $0 s|d|l [featureName]";

esac
