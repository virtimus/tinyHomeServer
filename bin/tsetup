#!/bin/bash

e=0
msg=''
thsHome=/ths/tinyHomeServer

handleError(){
  if [ $e -ne 0 ];then echo "[ths-shell-init] step $tstep error $e" && exit; fi && ((tstep++))
}

handleHeader(){
	echo "[tsetup] $msg ..."
}

handleEnd(){
	echo "[tsetup] END."
}



case "$1" in

  features/jupyter)
    source $thsHome/features/jupyter/ths-setup
		;;

	*)
			echo "Basic usage: $0 [featureName]";

esac
